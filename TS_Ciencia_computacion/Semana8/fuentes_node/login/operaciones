create database usuarios;

create table `users` (`id` int(11) NOT NULL PRIMARY KEY AUTO_INCREMENT,`nickname` varchar(80) NOT NULL,`nombre` varchar(80) NOT NULL,`password` varchar(80) NOT NULL);

